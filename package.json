{
  "name": "baseballhackathon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install:all": "tsx scripts/install-all.ts",
    "install:python": "npm run install:python:pose && npm run install:python:drills && npm run install:python:blast",
    "install:python:pose": "cd pose-detection-service && pip install -r requirements.txt",
    "install:python:drills": "cd drill-recommender && pip install -r requirements.txt",
    "install:python:blast": "cd blast-connector && pip install -r requirements.txt",
    "dev": "next dev --webpack",
    "dev:gateway": "tsx backend-gateway/index.ts",
    "dev:pose": "cd pose-detection-service && python app.py",
    "dev:pose:demo": "cd pose-detection-service && DEMO_MODE=true python app.py",
    "dev:pose:test": "cd pose-detection-service && TEST_MODE=true python app.py",
    "dev:pose:gunicorn": "cd pose-detection-service && gunicorn -w 4 -b 0.0.0.0:5000 app:app",
    "dev:drills": "cd drill-recommender && python app.py",
    "dev:drills:demo": "cd drill-recommender && DEMO_MODE=true python app.py",
    "dev:drills:test": "cd drill-recommender && TEST_MODE=true python app.py",
    "dev:drills:gunicorn": "cd drill-recommender && gunicorn -w 4 -b 0.0.0.0:5001 app:app",
    "seed:drills": "cd drill-recommender && python scripts/seed_drills.py",
    "dev:blast": "cd blast-connector && python app.py",
    "dev:blast:demo": "cd blast-connector && DEMO_MODE=true python app.py",
    "dev:blast:test": "cd blast-connector && TEST_MODE=true python app.py",
    "dev:blast:gunicorn": "cd blast-connector && gunicorn -w 4 -b 0.0.0.0:5002 app:app",
    "dev:storage": "cd storage-server && python app.py",
    "all": "npm run dev:all",
    "dev:all": "powershell -ExecutionPolicy Bypass -File scripts/start-all.ps1",
    "dev:all:unix": "bash scripts/start-all.sh",
    "start:all": "tsx scripts/run-and-check.ts",
    "start:dev": "concurrently -n \"Gateway,Blast,Frontend\" -c \"blue,green,yellow\" \"npm run dev:gateway\" \"npm run dev:blast\" \"npm run dev\"",
    "start:dev:full": "concurrently -n \"Gateway,Pose,Drills,Blast,Frontend\" -c \"blue,cyan,magenta,green,yellow\" \"npm run dev:gateway\" \"npm run dev:pose\" \"npm run dev:drills\" \"npm run dev:blast\" \"npm run dev\"",
    "run:check": "tsx scripts/run-and-check.ts",
    "dev:ngrok:backend": "ngrok start baseball-backend --config ngrok.yml",
    "dev:ngrok:frontend": "ngrok start baseball-frontend --config ngrok.yml",
    "dev:ngrok": "ngrok start baseball-backend --config ngrok.yml",
    "install:models": "bash scripts/install-models.sh",
    "install:models:python": "python3 scripts/install-models.py",
    "create-pr": "bash scripts/create-pr.sh",
    "pr": "bash scripts/create-pr.sh",
    "push-pr": "bash scripts/push-and-pr.sh",
    "push:pr": "bash scripts/push-and-pr.sh",
    "validate:env": "python scripts/validate-env.py",
    "test:mongodb": "tsx scripts/test-mongodb.ts",
    "test:swing": "tsx scripts/test-swing-detection.ts",
    "test:swing:py": "python scripts/test-swing-detection.py",
    "test:swing:simple": "python scripts/test-swing-detection-simple.py",
    "check:bat": "python scripts/check-bat-connection.py",
    "swing:detect": "python scripts/detect_swings.py",
    "encode:password": "tsx scripts/encode-mongodb-password.ts",
    "postinstall": "npm run install:models || echo 'Model installation skipped (will download on first use)'",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "replay:elevenlabs": "tsx scripts/replay-elevenlabs.ts"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@tensorflow-models/pose-detection": "^2.1.3",
    "@tensorflow/tfjs": "^4.22.0",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "form-data": "^4.0.0",
    "framer-motion": "^12.23.24",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "lucide-react": "^0.552.0",
    "mongodb": "^6.20.0",
    "multer": "^2.0.2",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "three": "^0.181.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
